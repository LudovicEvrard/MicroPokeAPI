  "effect_changes": [
    {
      "effect_entries": [
        {
          "effect": "Hat außerhalb vom Kampf keinen Effekt.",
          "language": {
            "name": "de",
            "url": "/language/6/"
          }
        },
        {
          "effect": "Has no overworld effect.",
          "language": {
